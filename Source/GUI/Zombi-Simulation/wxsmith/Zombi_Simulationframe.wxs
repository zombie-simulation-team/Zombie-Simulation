<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="Zombi_SimulationFrame">
		<icon>./icons/biohazard.png</icon>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="wxPanel" name="ID_BACKGROUND_PANEL" variable="BackgroundPanel" member="yes">
					<size>700,421</size>
					<bg>wxSYS_COLOUR_BTNSHADOW</bg>
					<handler function="OnBackgroundPanelPaint" entry="EVT_PAINT" />
					<object class="wxPanel" name="ID_CONFIG_PANEL" variable="ConfigPanel" member="yes">
						<pos>0,320</pos>
						<size>700,88</size>
						<object class="wxButton" name="ID_STAR_BUTTON" variable="StartButton" member="yes">
							<label>Start</label>
							<pos>24,32</pos>
							<handler function="OnStartButtonClick" entry="EVT_BUTTON" />
						</object>
					</object>
					<object class="wxPanel" name="ID_DISPALY_PANEL" variable="DisplayPanel" member="yes">
						<pos>0,0</pos>
						<size>700,300</size>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENUITEM1" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
