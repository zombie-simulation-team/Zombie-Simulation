<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="Zombi_SimulationFrame">
		<title>Zombie Simulation</title>
		<icon>./icons/biohazard.png</icon>
		<pos>-1,-1</pos>
		<size>710,460</size>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="wxPanel" name="ID_BACKGROUND_PANEL" variable="BackgroundPanel" member="yes">
					<size>700,-1</size>
					<object class="wxPanel" name="ID_CONFIG_PANEL" variable="ConfigPanel" member="yes">
						<pos>0,300</pos>
						<size>700,104</size>
						<bg>wxSYS_COLOUR_BTNFACE</bg>
						<style>wxSIMPLE_BORDER</style>
						<object class="wxButton" name="ID_STAR_BUTTON" variable="StartButton" member="yes">
							<label>Start</label>
							<pos>504,32</pos>
							<size>150,50</size>
							<bg>#A1D14B</bg>
							<handler function="OnStartButtonClick" entry="EVT_BUTTON" />
						</object>
						<object class="wxSpinCtrl" name="ID_ZOMBIE_SPINCTRL" variable="ZombieSpinCtrl" member="yes">
							<value>10</value>
							<min>2</min>
							<pos>152,20</pos>
							<size>56,27</size>
							<handler function="OnZombieSpinCtrlChange" entry="EVT_SPINCTRL" />
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
							<label>Zombies %</label>
							<pos>152,2</pos>
						</object>
						<object class="wxSpinCtrl" name="ID_TRAP_SPINCTRL" variable="TrapSpinCtrl" member="yes">
							<value>5</value>
							<max>10</max>
							<pos>264,20</pos>
							<size>56,27</size>
							<handler function="OnTrapSpinCtrlChange" entry="EVT_SPINCTRL" />
						</object>
						<object class="wxSpinCtrl" name="ID_RESOURCE_SPINCTRL" variable="ResourceSpinCtrl" member="yes">
							<value>10</value>
							<max>20</max>
							<pos>264,68</pos>
							<size>56,27</size>
							<handler function="OnResourceSpinCtrlChange" entry="EVT_SPINCTRL" />
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
							<label>Humans %</label>
							<pos>152,50</pos>
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
							<label>Traps %</label>
							<pos>264,2</pos>
							<size>64,17</size>
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="yes">
							<label>Resources %</label>
							<pos>264,50</pos>
						</object>
						<object class="wxSpinCtrl" name="ID_HUMAN_SPINCTRL" variable="HumanSpinCtrl" member="yes">
							<value>30</value>
							<min>5</min>
							<pos>152,68</pos>
							<size>56,27</size>
							<handler function="OnHumanSpinCtrlChange" entry="EVT_SPINCTRL" />
						</object>
						<object class="wxSpinCtrl" name="ID_CONTINENT_SPINCTRL" variable="ContinentSpinCtrl" member="yes">
							<value>4</value>
							<min>1</min>
							<max>6</max>
							<pos>380,20</pos>
							<size>63,27</size>
							<handler function="OnContinentSpinCtrlChange" entry="EVT_SPINCTRL" />
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText5" member="yes">
							<label>Continents</label>
							<pos>380,2</pos>
						</object>
						<object class="wxSpinCtrl" name="ID_SPINCTRL1" variable="SpeedCtrl" member="yes">
							<value>1</value>
							<min>1</min>
							<max>10</max>
							<pos>380,68</pos>
							<size>64,27</size>
							<handler function="OnSpeedCtrlChange" entry="EVT_SPINCTRL" />
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText6" member="yes">
							<label>Speed</label>
							<pos>380,50</pos>
						</object>
						<object class="wxGauge" name="ID_GAUGE" variable="ProgressGauge" member="yes">
							<pos>552,41</pos>
							<size>141,28</size>
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT7" variable="StaticText7" member="yes">
							<label>Progress</label>
							<pos>480,48</pos>
						</object>
						<object class="wxButton" name="ID_BUTTON1" variable="LoadButton" member="yes">
							<label>Load</label>
							<pos>32,16</pos>
							<handler function="OnLoadButtonClick" entry="EVT_BUTTON" />
						</object>
						<object class="wxButton" name="ID_BUTTON2" variable="SaveButton" member="yes">
							<label>Save</label>
							<pos>32,56</pos>
							<handler function="OnSaveButtonClick" entry="EVT_BUTTON" />
						</object>
						<object class="wxStaticText" name="ID_STATICTEXT8" variable="StaticText8" member="yes">
							<label>Humans vs Zombies:</label>
							<pos>475,8</pos>
						</object>
						<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="ScoreTextCtrl" member="yes">
							<value>0 - 0</value>
							<maxlength>10</maxlength>
							<pos>615,5</pos>
							<size>50,20</size>
							<style>wxTE_READONLY|wxTE_CENTRE</style>
						</object>
					</object>
					<object class="wxPanel" name="ID_DISPALY_PANEL" variable="DisplayPanel" member="yes">
						<pos>0,0</pos>
						<size>700,300</size>
						<bg>#05B7F7</bg>
						<object class="wxStaticBitmap" name="ID_STATICBITMAP1" variable="WelcomeScreen" member="yes">
							<bitmap>/home/ubuntu/Documents/Zombie-Simulation/GUI/icons/StartScreen.PNG</bitmap>
							<pos>0,0</pos>
							<size>700,300</size>
						</object>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
